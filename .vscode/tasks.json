{
    "tasks": [
        {
            "type": "shell",
            "label": "mkdir_debug",
            "command": "mkdir",
            "args": [
                "-p",
                "bin/debug/${relativeFileDirname}"
            ]
        },
        {
            "type": "cppbuild",
            "label": "gcc",
            "command": "/usr/bin/gcc",
            "dependsOn": [
                "mkdir_debug"
            ],
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${file}",
                "-o",
                "${cwd}/bin/debug/${relativeFileDirname}/${fileBasenameNoExtension}"
            ],
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}
